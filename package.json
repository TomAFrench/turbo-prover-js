{
  "name": "@noir-lang/turbo-prover",
  "version": "0.1.0",
  "main": "index.js",
  "license": "GPL-2.0",
  "scripts": {
    "build": "yarn clean && tsc -p tsconfig.dest.json && node ./package.js",
    "build:dev": "tsc -p tsconfig.dest.json --watch",
    "clean": "rm -rf ./dest ./.cache",
    "test": "jest --passWithNoTests",
    "formatting": "prettier --check ./src && eslint --max-warnings 0 ./src"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": ".*\\.test\\.ts$",
    "rootDir": "./src",
    "testTimeout": 30000,
    "slowTestThreshold": 60
  },
  "browserslist": [
    "since 2017-06"
  ],
  "dependencies": {
    "@noir-lang/barretenberg": "2.19.0"
  },
  "devDependencies": {
    "@types/node": "^15.6.2",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^27.0.3",
    "prettier": "^2.6.2",
    "ts-jest": "^27.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.4.4"
  }
}
